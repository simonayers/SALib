| Rough makefile for building SALib library with G++

WimpSlot -min 20M

Dir <Obey$Dir>

echo Compiling source files...
echo
g++ -c -mlibscl -IOSLib: -o windowobserver.o windowobserver.cc
g++ -c -mlibscl -IOSLib: -o iconbar.o iconbar.cc
g++ -c -mlibscl -IOSLib: -o iconbuilder.o iconbuilder.cc
g++ -c -mlibscl -IOSLib: -o menu.o menu.cc
g++ -c -mlibscl -IOSLib: -o menuselectionobserver.o menuselectionobserver.cc
g++ -c -mlibscl -IOSLib: -o mouseclickobserver.o mouseclickobserver.cc
g++ -c -mlibscl -IOSLib: -o observer.o observer.cc
g++ -c -mlibscl -IOSLib: -o pointer.o pointer.cc
g++ -c -mlibscl -IOSLib: -o task.o task.cc
g++ -c -mlibscl -IOSLib: -o utilities.o utilities.cc
g++ -c -mlibscl -IOSLib: -o window.o window.cc
g++ -c -mlibscl -IOSLib: -o windowbuilder.o windowbuilder.cc

echo Archiving object files into static library...
echo
ar rcs libSALib.a windowobserver.o iconbar.o iconbuilder.o menu.o menuselectionobserver.o mouseclickobserver.o observer.o pointer.o task.o utilities.o window.o windowbuilder.o

echo Deleting object files...
Wipe o.* ~CFR~V